   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread(\"robot.jpg\",0)\n",
    "row,col = img.shape\n",
    "\n",
    "M= cv2.getRotationMatrix2D((col/5,row/3),180,1)\n",
    "\n",
    "dst = cv2.warpAffine(img,M,(col,row))\n",
    "\n",
    "cv2.imshow(\"dst\",dst)"
   ]
  }
